{
  "compilerOptions": {
    "module": "nodenext",
    "target": "esnext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "verbatimModuleSyntax": false,
    "isolatedModules": true,
    "exactOptionalPropertyTypes": true,
    "moduleDetection": "force"
  }
}
